{{- $paginator := (.Site.Pages)  -}}
{{- $len := (len $paginator) -}}
{
  "static_urls":[
    {{ range $i, $element := $paginator -}}
    {{- if or .Params.available .Params.summaryAvailable -}}
    {{- if ne .Params.includeInSitemap false -}}
      {{- $link := .Permalink | replaceRE "[^/]*$" "" -}}
      {{- if eq $link "/book/" -}}
        "/book/contents/"
      {{- else -}}
        "{{ $link }}"
      {{- end -}}
      {{- if ne (add $i 1) $len }},{{ end }}
    {{ end -}}
    {{- end -}}
    {{- end -}}
  ]
}

